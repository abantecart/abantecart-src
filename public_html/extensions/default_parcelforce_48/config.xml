        </item>
	    <item id="default_parcelforce_48_display_weight">
            <type>checkbox</type>
		    <default_value>1</default_value>
        </item>
	    <item id="default_parcelforce_48_display_insurance">
		    <type>checkbox</type>
		    <default_value>1</default_value>
        </item>
